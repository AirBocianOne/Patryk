#include <iostream>

using namespace std;

int main()
{
    int testy;
    cin>>testy;
     while (testy--)
     {
          int ile;
        cin>>ile;
        int liczby[ile];

        for(int i=0; i<ile; i++)
        {
            cin>>liczby[i];
        }
        for(int i=ile-1; i>=0; i--)
        {
            cout<<liczby[i]<<" ";
        }
        cout<<endl;
     }
    return 0;
}
